{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console":"off",
        "import/new-line-after-import":"off",
        "import/no-extraneous-dependencies":"off",
        "no-unused-vars":"off",
        "max-len":"off",
        "indent": ["error", 4]
    }
}